{
    "name": "@tstelzer/coverage",
    "version": "0.0.1-alpha.1",
    "type": "module",
    "license": "MIT",
    "sideEffects": [],
    "publishConfig": {
        "directory": "dist"
    },
    "tags": [
        "typescript",
        "performance",
        "confusion-matrix",
        "metrics"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tstelzer/performance-metrics.git"
    },
    "scripts": {
        "build": "tsc --build tsconfig.build.json",
        "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
        "clean": "rm -rf build dist .tsbuildinfo",
        "prepublish": "node build/esm/src/build.js",
        "build-all": "pnpm run clean && pnpm run build && pnpm run build-cjs && pnpm run prepublish"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.4",
        "@babel/core": "^7.23.6",
        "@babel/plugin-transform-export-namespace-from": "^7.23.4",
        "@babel/plugin-transform-modules-commonjs": "^7.23.3",
        "@creative-introvert/eslint-config": "^0.3.2",
        "@creative-introvert/prettier-config": "^0.2.0",
        "@types/node": "^20",
        "app-root-path": "^3.1.0",
        "type-fest": "^4.8.2",
        "typescript": "^5.3.2"
    },
    "dependencies": {
        "@effect/platform": "^0.46.0",
        "@effect/platform-node": "^0.44.8",
        "@effect/schema": "^0.63.0",
        "ansi-colors": "^4.1.3",
        "effect": "2.4.0"
    }
}
